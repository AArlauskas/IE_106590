<Configuration status="debug" name="`lab1FileLogger`" packages="">
<Appenders>
    <File name="FileLogger" fileName="lab1.log">
        <PatternLayout pattern="%d{yyyy-mm-dd HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n"/>
    </File>
</Appenders>
    <loggers>
        <root level="debug">
            <appender-ref ref="FileLogger" level="info"/>
        </root>
    </loggers>
</Configuration>